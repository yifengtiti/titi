<!--pages/test/index.wxml-->
<view class="item_ul">
  <view class="item_li item_boder">
    <view class="item_img">
        <image src="/images/recommendeds.png"  mode='widthFix'></image>
    </view>
    <view  class="item_left">不做他的推荐人</view>
    <view class="item_right">
      <switch bindchange="recommend1" checked='{{satte1}}'/>
    </view>
  </view>
  <view class="item_li item_boder item_boders">
    <view  class="item_left">不能将我推荐给别人</view>
    <view class="item_right">
      <switch  bindchange="recommend2" checked='{{satte2}}'/>
    </view>
  </view>
  <view class="item_li item_boder item_boders">
  
    <view  class="item_left">不接受他的推荐</view>
    <view class="item_right">
      <switch bindchange="recommend3" checked='{{satte3}}'/>
    </view>
  </view>
  <view class="item_li">
  <view class="item_img">
        <image src="/images/blacklist.png"  mode='widthFix'></image>
    </view>
    <view  class="item_left">黑名单</view>
    <view class="item_right">
      <switch  bindchange="switch2Change" checked='{{satte}}'/>
    </view>
  </view>
  
  <view class="item_li" bindtap="complaints">
  <view class="item_img">
        <image src="/images/complaints.png"  mode='widthFix'></image>
    </view>
    <view  class="item_left" >投诉他</view>
  </view>
  <view class="item_li" bindtap="record">
    <view class="item_img">
        <image src="/images/delete.png"  mode='widthFix'></image>
    </view>
    <view  class="item_left" >删除人脉</view>
  </view>
</view>
